{
  "name": "parking-assistant",
  "version": "0.1.0",
  "description": "Find parking space by Google Assistant",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=production && node dist/app.js",
    "dev": " set NODE_ENV=development && nodemon src/app.ts",
    "build": "tsc -p tsconfig.production.json",
    "prod": "now --prod dist -A now.json --token=$NOW_TOKEN",
    "update": "node dist/utils/parks.js"
  },
  "author": "Kiros Choi",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@sentry/integrations": "^5.12.0",
    "@sentry/node": "^5.12.2",
    "actions-on-google": "^2.12.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
